<h1>Listing project_necessary_dates</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Comment</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_necessary_dates.each do |project_necessary_date| %>
      <tr>
        <td><%= project_necessary_date.date %></td>
        <td><%= project_necessary_date.comment %></td>
        <td><%= project_necessary_date.project %></td>
        <td><%= link_to 'Show', project_necessary_date %></td>
        <td><%= link_to 'Edit', edit_project_necessary_date_path(project_necessary_date) %></td>
        <td><%= link_to 'Destroy', project_necessary_date, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Necessary date', new_project_necessary_date_path %>
