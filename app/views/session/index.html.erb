<h1>Session#index</h1>
<p>Find me in app/views/session/index.html.erb</p>

<%= form_tag '/login' do %>
  <div>
    <%= label_tag "账号" %>
    <%= text_field_tag :account, params[:account], required: true, autofocus: true%>
  </div>        
  <div>
    <%= label_tag "密码" %>
    <%= password_field_tag :password, params[:password], required: true%>
  </div>
  <%= submit_tag "登录", id: "button"%>
<% end %>
