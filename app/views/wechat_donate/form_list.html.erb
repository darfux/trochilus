<div class="common-list">
<table>
  <tr>
    <th>捐款人</th>
    <th>数额</th>
    <th>邮箱</th>
    <th>电话</th>
    <th>地址</th>

    <th>邮编</th>
    <th>单位</th>
    <th>在校信息</th>
    <th>留言</th>
  </tr>

<% @formdata.each do |d| %>
  <tr>
    <td><%= d.donator %></td>
    <td><%= d.amount/100.0 %></td>
    <td><%= d.email %></td>
    <td><%= d.phone %></td>
    <td><%= d.address %></td>

    <td><%= d.postcode %></td>
    <td><%= d.company %></td>
    <td><%= d.school_info %></td>
    <td><%= d.message %></td>
  </tr>
<% end %>
</table>
</div>